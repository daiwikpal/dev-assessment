{
  "dependencies": {
    "axios": "^1.4.0",
    "concurrently": "^8.2.0",
    "cors": "^2.8.5",
    "react-router-dom": "^6.15.0",
    "swr": "^2.2.1"
  },
  "scripts": {
    "postinstall": "concurrently yarn:postinstall:*",
    "postinstall:backend": "cd backend/ && yarn install",
    "postinstall:frontend": "cd frontend/ && yarn install",
